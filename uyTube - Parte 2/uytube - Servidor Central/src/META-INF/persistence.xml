<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="UyTubePU" transaction-type="RESOURCE_LOCAL">
    <!-- Persistence provider -->
    <!-- Entity classes -->
    <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
    <class>logica.Usuario</class>
    <class>logica.Video</class>
    <class>logica.Categoria</class>
    <class>logica.Canal</class>
    <class>logica.Valoracion</class>
    <class>logica.ListaDeReproduccion</class>
    <class>logica.ListaDeReproduccion_PorDefecto</class>
    <class>logica.Comentario</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:hsqldb:hsql://localhost/uytubedb"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="org.hsqldb.jdbc.JDBCDriver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
      <property name="hibernate.dialect" value="org.hibernate.dialect.HSQLDialect"/>
    </properties>
  </persistence-unit>
</persistence>